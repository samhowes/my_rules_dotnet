load("@my_rules_dotnet//dotnet:defs.bzl", "dotnet_library")

dotnet_library(
    name = "contentglobs",
    srcs = glob(["*.cs"]),
    content = glob(["foo/**"]) + glob(
        ["config/**"],
        exclude = ["*.json"],
    ) + ["appsettings.json"],
    sdk = "Microsoft.NET.Sdk",
    target_framework = "net5.0",
    visibility = ["//visibility:public"],
)
